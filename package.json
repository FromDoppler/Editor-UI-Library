{
  "name": "editor-ui-system",
  "version": "1.0.0",
  "description": "Editor Ui Library",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve:dev",
    "serve:dev": "npm run build:dev && webpack-dev-server --inline --open",
    "serve:prod": "npm run build:prod && webpack-dev-server --inline --open",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "build:dev": "npm run clean-dist && webpack --config webpack.config.dev.js",
    "build:prod": "npm run clean-dist && webpack --config webpack.config.prod.js",
    "verify": "npm prettier-check && npm run eclint-check",
    "eclint-fix": "eclint fix \"**/*\"",
    "eclint-check": "eclint check \"**/*\"",
    "prettier-fix": "prettier --write \"**/**\"",
    "prettier-check": "prettier --check \"**/**\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^3.5.3",
    "eclint": "^2.8.1",
    "file-loader": "^6.0.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.3.0",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^5.0.0",
    "post-loader": "^2.0.0",
    "prettier": "^2.0.5",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.5",
    "svg-sprite-loader": "^4.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.0",
    "webpack-merge": "^4.2.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
